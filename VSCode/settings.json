{
  "workbench.colorTheme": "Dracula Soft",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.startupEditor": "none",
  "workbench.experimental.layoutControl.enabled": true,
  "workbench.experimental.layoutControl.type": "toggles",
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "support.constant.classname.twin",
        "settings": {
          "foreground": "#f2fb9d"
        }
      }
    ]
  },
  "workbench.colorCustomizations": {
    "editorBracketHighlight.foreground1": "#ffc14ed7",
    "editorBracketHighlight.foreground2": "#ff6eb2d7",
    "editorBracketHighlight.foreground3": "#45a8f9d7",
    "editorBracketHighlight.foreground4": "#B084EBd7",
    "editorBracketHighlight.foreground5": "#36f2ffd7",
    "editorBracketHighlight.foreground6": "#72faafd7",
    "editorBracketHighlight.unexpectedBracket.foreground": "#ff184a"
  },
  "material-icon-theme.hidesExplorerArrows": true,
  "material-icon-theme.folders.color": "#ead2ac",
  "editor.fontSize": 13,
  "editor.fontFamily": "'Iosevka', 'Ubuntu Mono', 'DM Mono', Consolas, 'Courier New', monospace",
  "editor.tabSize": 2,
  "editor.renderWhitespace": "boundary",
  "editor.wordWrap": "bounded",
  "editor.formatOnSave": true,
  "editor.wordWrapColumn": 90,
  "editor.quickSuggestions": {
    "strings": true
  },
  "files.autoSave": "onFocusChange",
  "security.workspace.trust.untrustedFiles": "newWindow",
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[postcss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[dockercompose]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "search.exclude": {
    "**/node_modules": true,
    "package*.json": true,
    "*.lock": true
  },
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "git.autofetchPeriod": 300,
  "git.pruneOnFetch": true,
  "git.enableSmartCommit": true,
  "git.autofetch": true,
  "git.enableCommitSigning": true,
  "gitlens.views.remotes.pullRequests.enabled": false,
  "gitlens.codeLens.enabled": false,
  "gitlens.advanced.messages": {
    "suppressCreatePullRequestPrompt": true
  },
  "gitlens.menus": {
    "editor": {
      "blame": false,
      "clipboard": true,
      "compare": true,
      "history": false,
      "remote": false
    },
    "editorGroup": false,
    "editorTab": {
      "clipboard": true,
      "compare": true,
      "history": true,
      "remote": true
    },
    "explorer": {
      "clipboard": true,
      "compare": true,
      "history": true,
      "remote": true
    },
    "scm": {
      "authors": true
    },
    "scmGroupInline": {
      "stash": true
    },
    "scmGroup": {
      "compare": true,
      "openClose": true,
      "stash": true
    },
    "scmItem": {
      "clipboard": true,
      "compare": true,
      "history": true,
      "remote": false,
      "stash": true
    }
  },
  "php.suggest.basic": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "security.workspace.trust.emptyWindow": false,
  "editor.guides.bracketPairs": true,
  "editor.bracketPairColorization.enabled": true,
  "projectManager.git.baseFolders": ["D:\\projects", "D:\\1-dev_tools"],
  "projectManager.vscode.baseFolders": ["D:\\projects"],
  "projectManager.tags": [],
  "projectManager.git.ignoredFolders": [
    "node_modules",
    "out",
    "typings",
    "test",
    ".haxelib",
    "build"
  ],
  "files.associations": {
    ".env.local": "ini"
  },
  "screencastMode.onlyKeyboardShortcuts": true,
  "redhat.telemetry.enabled": false,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "docker.volumes.groupBy": "CreatedTime",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "tabnine.experimentalAutoImports": true,
  "autoimport.filesToScan": "**/*.{js,jsx,ts,tsx}",
  "css.validate": false,
  "typescript.suggest.paths": false,
  "javascript.suggest.paths": false,
  "tailwindcss.colorDecorators": "off"
}
